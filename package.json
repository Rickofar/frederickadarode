    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/router": "^17.0.2",
    "font-awesome": "^4.7.0",
    "gsap": "^3.12.2",
    "ngx-lightbox": "^3.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "vite": "^5.0.12",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1700.10",
    "@angular-devkit/build-angular": "^17.0.10",
